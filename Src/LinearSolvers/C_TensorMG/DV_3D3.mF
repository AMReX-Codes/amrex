               dudxw = <* FA[ dudx[i-1+half,j,k] //.  allDerivAllUV ] *>

               dvdyn = <* FA[ dvdy[i,j+half,k] //.    allDerivAllUV ] *>
               dvdys = <* FA[ dvdy[i,j-1+half,k] //.  allDerivAllUV ] *>

               dwdzt = <* FA[ dwdz[i,j,k+half]   //.  allDerivAllUV ] *>
               dwdzb = <* FA[ dwdz[i,j,k-1+half] //.  allDerivAllUV ] *>

